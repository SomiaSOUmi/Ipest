<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IpestV2.Tech7">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="New Trap" x:Name="integrate" Style="{StaticResource buttonStyle}" Clicked="Integrate_Clicked" HorizontalOptions="Center" VerticalOptions="Center" />
            <Button Text="Migrate existing trap" x:Name="migrate" Style="{StaticResource buttonStyle}" Clicked="Migrate_Clicked" HorizontalOptions="Center" VerticalOptions="Center" />

        </StackLayout>
    </ContentPage.Content>
</ContentPage>