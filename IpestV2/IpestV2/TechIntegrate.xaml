<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IpestV2.TechIntegrate">
    <ContentPage.Content>
        <StackLayout>
            <Editor Placeholder="Please enter location data" HeightRequest ="200" WidthRequest="400" BackgroundColor="LightBlue" HorizontalOptions="Center" VerticalOptions="Center" />
            <Editor Placeholder="Meta data" HeightRequest ="300" WidthRequest="400" BackgroundColor="LightBlue" HorizontalOptions="Center" VerticalOptions="Center" />
            <Button  Text="Submit" Clicked="Button_Clicked"/>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>